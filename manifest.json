  {
    "manifest_version": 2,

    "name": "aaSimple Pomodoro Timer DEV",
    "description": "Set intervals of work and rest to maximize productivity",
    "version": "0.0.0.6",
    "author": "Richard Cai",
    "options_page": "options.html",

    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["options.js"],
      "css": ["style.css"]
    }],

    "permissions": [
        "storage"
    ],

    "browser_action": {
      "default_popup": "popup.html"
    }
  }
